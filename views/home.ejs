<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GameBombay</title>
    <link rel="stylesheet" href="/stylesheets/home.css">

</head>
<body>
    <div class="header">
        <div class="logoD">
            <h2 class="logo">GAME BOMBAY</h2>
        </div>
    </div>
        <div class="links">
        <a href="/game">FARIDABAAD  Result Chart</a><br>
        <a href="/game">GHAZIABAD  Result Chart</a><br>
        <a href="/game">GALI  Result Chart</a><br>
        <a href="/game">DESAWAR  Result Chart</a><br>
    </div>

    <table>
        <thead>
            <tr>
                <td>Date</td>
                <td>Game 3pm</td>
                <td>Game 6pm</td>
                <td>Game 9pm</td>
                <td>Game 12am</td>
                <td>Game 6am</td>
            </tr>
        </thead>
        <tbody>
            <% let recData= data %>
            <% let reverseArray = recData.reverse() %>
            <% reverseArray.forEach((take)=>{ %>
                <tr>
                    <td><%= take.date %></td>
                    <td><%= take.bombay_3pm %></td>
                    <td><%= take.bombay_6pm %></td>
                    <td><%= take.bombay_9pm %></td>
                    <td><%= take.bombay_12am %></td>
                    <td><%= take.bombay_6am %></td>
                </tr>
            <% }) %>
        </tbody>
        
    </table>


</body>
</html>